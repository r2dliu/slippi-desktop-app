{
  "name": "slippi-launcher",
  "productName": "Slippi Launcher",
  "description": "Launch Slippi Online, browse and watch saved replays",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "dev": "electron-webpack dev",
    "build": "electron-webpack",
    "package": "yarn run build && electron-builder",
    "package:dir": "yarn run package --dir -c.compression=store -c.mac.identity=null",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn run lint --fix",
    "postinstall": "patch-package && electron-rebuild -f -w better-sqlite3"
  },
  "dependencies": {
    "@apollo/client": "^3.3.4",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@slippi/slippi-js": "^5.1.1",
    "adm-zip": "^0.5.1",
    "better-sqlite3": "^7.1.2",
    "comlink": "^4.3.0",
    "electron-context-menu": "^2.3.0",
    "electron-log": "^4.3.1",
    "electron-settings": "^4.0.2",
    "firebase": "^8.1.2",
    "fs-extra": "^9.0.1",
    "graphql": "^15.4.0",
    "immer": "^8.0.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.2.4",
    "react-router-dom": "^5.2.0",
    "react-virtualized-auto-sizer": "^1.0.3",
    "react-window": "^1.8.6",
    "source-map-support": "^0.5.16",
    "styled-components": "^5.2.1",
    "typeface-roboto": "^1.1.13",
    "zustand": "^3.2.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/better-sqlite3": "^5.4.1",
    "@types/lodash": "^4.14.165",
    "@types/moment": "^2.13.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "comlink-loader": "^2.0.0",
    "dotenv-webpack": "^6.0.0",
    "electron": "7.3.3",
    "electron-builder": "^22.9.1",
    "electron-notarize": "^1.0.0",
    "electron-rebuild": "^2.3.4",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "node-sass": "^5.0.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^10.1.0",
    "typescript": "3.8.3",
    "webpack": "~4.42.1"
  },
  "author": {
    "name": "Jas Laferriere",
    "email": "jas.laferriere@gmail.com",
    "url": "https://github.com/jlaferri"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "yarn lint --quiet"
    }
  }
}
